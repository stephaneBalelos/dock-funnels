    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@primevue/auto-import-resolver": "^4.3.5",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
